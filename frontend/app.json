{
  "expo": {
    "name": "Onion Meals",
    "slug": "onion-meal-planner",
    "version": "0.0.8",
    "orientation": "portrait",
    "icon": "./assets/images/onion-meal-planner-icon.png",
    "scheme": "frontend",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "splash": {
      "image": "./assets/images/onion-meal-planner-icon.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/onion-meal-planner-icon.png",
        "backgroundColor": "#ffffff"
      },
      "edgeToEdgeEnabled": true,
      "predictiveBackGestureEnabled": false,
      "package": "com.mrdziubek.onionmealplanner"
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/onion-meal-planner-icon.png"
    },
    "plugins": ["expo-router", "expo-secure-store"],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {},
      "eas": {
        "projectId": "e53d59bb-bcc9-4774-b3a8-dc21ea7274b4"
      }
    }
  }
}
